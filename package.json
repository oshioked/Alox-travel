{
  "name": "alox-travel",
  "version": "1.0.0",
  "description": "a pwa for a travel products brand",
  "engines": {
    "node": "13.3.0",
    "nom": "6.13.1"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail\"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "redux-logger": "^3.0.6",
    "stripe": "^8.49.0"
  },
  "author": "Apelinade Developers",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.2.0"
  }
}
